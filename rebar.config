{deps, [
    {amoc, {git, "https://github.com/esl/amoc", {branch, "amoc_core"}}},
    {docsh, "0.7.2"},
    {cowboy, "2.10.0"},
    {rfc3339, "0.9.0"},
    {jsx, "3.1.0"},
    {jesse, "1.7.11"}
]}.

{ profiles, [
    {test, [
        {deps, [
            {meck, "0.9.2"},
            {proper, "1.4.0"},
            {fusco, "0.1.1"}
        ]}
    ]}
]}.

{xref_checks, [
    undefined_function_calls,
    undefined_functions,
    locals_not_used,
    deprecated_function_calls,
    deprecated_functions
]}.

{dialyzer, [
    {warnings, [unknown]},
    {plt_extra_apps, [ranch, cowboy]}
]}.
