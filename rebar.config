{deps, [
    {ranch, "2.1.0"},
    {cowboy, "2.12.0"},
    {jsx, "3.1.0"},
    {jesse, "1.8.0"}
]}.

{dialyzer, [
    {warnings, [unknown]},
    {plt_extra_apps, [cowboy, cowlib, ranch]}]}.

{xref_checks, [
    undefined_function_calls,
    undefined_functions,
    locals_not_used,
    deprecated_function_calls,
    deprecated_functions
]}.
